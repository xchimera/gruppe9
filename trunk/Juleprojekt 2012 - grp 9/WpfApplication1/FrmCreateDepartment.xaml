        <Label Content="Afdelingsnavn" Height="28" HorizontalAlignment="Left" Margin="12,46,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Afdelingsnummer" Height="28" HorizontalAlignment="Left" Margin="12,0,0,188" Name="label2" VerticalAlignment="Bottom" />
