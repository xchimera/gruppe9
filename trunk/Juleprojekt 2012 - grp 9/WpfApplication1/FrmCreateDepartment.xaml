        <Label Content="Post nummer" Height="28" HorizontalAlignment="Left" Margin="12,165,0,0" Name="label7" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="130,108,0,0" Name="txtStreetName" VerticalAlignment="Top" Width="120" TabIndex="3" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="130,167,0,0" Name="txtPostalCode" VerticalAlignment="Top" Width="120" TabIndex="5" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="130,137,0,0" Name="txtStreetNumber" VerticalAlignment="Top" Width="120" TabIndex="4" />
    </Grid>
</Window>
